  "description": "It's just my portfolio",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/http-server/bin/http-server dist/",
    "build": "node_modules/webpack/bin/webpack.js --progress; npm run autoprefixer",
    "build:production": "node_modules/webpack/bin/webpack.js --progress -p, npm run autoprefixer",
    "watch:dev": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base /dist",
    "autoprefixer": "node_modules/postcss-cli/bin/postcss --use autoprefixer -o dist/styles.css dist/*.css",
    "storybook": "start-storybook -s ./src/assets -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:davidjsalazarmoreno/portfolio.js.git"
  },
