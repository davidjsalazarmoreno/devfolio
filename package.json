{
  "name": "portfolio",
  "version": "0.0.1",
  "description": "It's just my portfolio",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/http-server/bin/http-server docs/",
    "watch": "webpack --progress --config webpack.config.js --mode=development --watch",
    "build:development": "webpack --progress --config webpack.config.js --mode=development; npm run autoprefixer",
    "build:production": "webpack --config webpack.config.js --progress --mode=production; npm run autoprefixer",
    "autoprefixer": "node_modules/postcss-cli/bin/postcss --use autoprefixer -o docs/styles.css docs/*.css",
    "storybook": "start-storybook -s ./src/assets -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:davidjsalazarmoreno/portfolio.js.git"
  },
  "keywords": [
